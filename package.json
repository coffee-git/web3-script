{
  "name": "web3-script",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "frontend:dev": "npm run dev --workspace=frontend -- -p 3003",
    "frontend:build": "npm run build --workspace=frontend",
    "frontend:start": "npm run build --workspace=frontend && npm run start --workspace=frontend -- -p 3001",
    "backend": "npm run start --workspace=backend",
    "dev": "concurrently \"npm run frontend:dev\" \"npm run backend\""
  }
}
